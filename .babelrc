{
    "presets": [
        ["@babel/env", {
            "targets": {
                "browsers": ["last 2 versions"],
                "node": "current"
            }
        }],
        "@babel/stage-3",
        "@babel/react",
        "@babel/typescript"
    ],
    "plugins": [
        ["module-resolver", {
            "extensions": [".js", ".jsx", ".ts", ".tsx"],
            "root": ["./src"]
        }]
    ]

}