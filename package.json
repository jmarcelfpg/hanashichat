    "redux-actions": "^2.3.0",
    "socket.io-client": "^2.1.0",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "@types/socket.io": "^1.4.33",
    "express": "^4.16.3",
    "socket.io": "^2.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
