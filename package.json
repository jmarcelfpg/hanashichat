{
  "name": "hanashichat",
  "version": "0.0.4",
  "description": "A chat to interact with new people or old friends",
  "main": "/dist/server/index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/server",
    "start-development": "nodemon dist/server --watch dist",
    "build": "webpack && tsc -p tsconfig.server.json",
    "build-watch": "concurrently \"webpack -w\" \"tsc -p tsconfig.server.json -w\" \"nodemon dist/server --watch dist\"",
    "build-server": "tsc -p tsconfig.server.json",
    "build-serverWatch": "tsc -p tsconfig.server.json -w",
    "build-client": "webpack -w",
    "build-clientWatch": "webpack"
  },
  "repository": "https://github.com/jmarcelfpg/hanashichat.git",
  "keywords": [
    "chat",
    "leaf",
    "leaves"
  ],
  "author": "jmarcelfpg <jmarcel_fpg@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmarcelfpg/hanashichat/issues"
  },
  "homepage": "https://github.com/jmarcelfpg/hanashichat#readme",
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.18",
    "@types/react-router": "^4.0.24",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.4",
    "@types/socket.io-client": "^1.4.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "jest-cli": "^22.4.3",
    "moment": "^2.22.1",
    "nodemon": "^1.17.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
